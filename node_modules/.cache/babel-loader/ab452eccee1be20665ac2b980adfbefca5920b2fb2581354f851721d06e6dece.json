{"ast":null,"code":"import { execute, trade } from \"@/api\";\nexport default {\n  name: \"TradePermit\",\n  data() {\n    return {\n      permit: {\n        id: null,\n        billNo: null,\n        businessID: null,\n        duration: null,\n        billTotal: null,\n        receiptAmount: null,\n        billBalance: null,\n        startDate: null,\n        endDate: null,\n        status: null,\n        printable: null,\n        brimsCode: null,\n        businessName: null,\n        certificateOfIncorporation: null,\n        pinNumber: null,\n        VATNumber: null,\n        plotNo: null,\n        physicalAddress: null,\n        businessActivity: null,\n        businessActivityDescription: null,\n        telephone1: null,\n        telephone2: null,\n        address: null,\n        postalCode: null,\n        town: null,\n        countyID: null,\n        subCountyID: null,\n        wardID: null,\n        dateCreated: null,\n        createdBy: null,\n        dateModified: null,\n        modifiedBy: null,\n        isActive: null,\n        latitude: null,\n        longitude: null\n      },\n      link: ''\n    };\n  },\n  mounted() {\n    let params = {};\n    try {\n      params = JSON.parse(localStorage['params']);\n      console.log('####' + params.businessID);\n      this.printTradePermit(params.businessID);\n    } catch (error) {\n      // ignore\n    }\n  },\n  methods: {\n    showDate() {\n      const dateObj = new Date();\n      const currentDate = dateObj.getDate() + \"/\" + dateObj.getMonth() + \"/\" + dateObj.getFullYear();\n      return currentDate;\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    printTradePermit(businessID) {\n      const data = new FormData();\n      data.append(\"function\", \"printTradePermit\");\n      data.append(\"businessID\", businessID);\n      execute(data, trade).then(res => {\n        if (res.data.success) {\n          this.permit = res.data.data.permit;\n          this.link = \"https://chart.googleapis.com/chart?chs=300x300&cht=qr&chl=Business ID: \" + this.permit.businessID + \"\\n \" + this.permit.businessName + \" start date: \" + this.permit.startDate + \" Expiry:\" + this.permit.endDate;\n        } else {\n          alert(res.data.data.message);\n        }\n      }).catch(e => {\n        alert(e.message);\n      });\n    },\n    mailTradePermit() {\n      const data = new FormData();\n      data.append(\"function\", \"mailTradePermit\");\n      data.append(\"businessID\", this.$route.params['data']);\n      execute(data, trade).then(res => {\n        alert(res.data.message);\n      }).catch(e => {\n        alert(e.message);\n      });\n    }\n  }\n};","map":{"version":3,"names":["execute","trade","name","data","permit","id","billNo","businessID","duration","billTotal","receiptAmount","billBalance","startDate","endDate","status","printable","brimsCode","businessName","certificateOfIncorporation","pinNumber","VATNumber","plotNo","physicalAddress","businessActivity","businessActivityDescription","telephone1","telephone2","address","postalCode","town","countyID","subCountyID","wardID","dateCreated","createdBy","dateModified","modifiedBy","isActive","latitude","longitude","link","mounted","params","JSON","parse","localStorage","console","log","printTradePermit","error","methods","showDate","dateObj","Date","currentDate","getDate","getMonth","getFullYear","formatPrice","value","val","toFixed","replace","toString","FormData","append","then","res","success","alert","message","catch","e","mailTradePermit","$route"],"sources":["C:\\My Projects\\Levetot\\Vue projects\\county biller green\\public\\PrintDocs\\Trade-Permit\\TradePermit.vue"],"sourcesContent":["<template>\r\n  <html>\r\n\r\n  <head>\r\n    <meta charset=\"utf-8\">\r\n    <title class=\"\">HOMABAY County Trade Permit</title>\r\n    <link href=\"https://fonts.googleapis.com/css?family=Aldrich|Fira+Sans:200,300,400,500,700,800,900|Norican&display=swap&subset=cyrillic\" rel=\"stylesheet\">\r\n    <link href=\"https://fonts.googleapis.com/css?family=Libre+Baskerville:400,400i,700|Open+Sans:300,400,600,700&display=swap\" rel=\"stylesheet\">\r\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\">\r\n    <link href=\"https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,600;0,700;0,800;1,300;1,400;1,600;1,700;1,800&display=swap\" rel=\"stylesheet\">\r\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\r\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\r\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\r\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\r\n    <link href=\"https://fonts.googleapis.com/css2?family=Pridi:wght@200;300;400;500;600;700&display=swap\" rel=\"stylesheet\">\r\n\r\n  </head>\r\n\r\n  <body>\r\n  <page size=\"A4\" class=\"p-2 container w-100 h-100\">\r\n\r\n    <h1 style=\"text-transform: uppercase;font-size: 137px;position: absolute;transform: rotate(45deg);top: 40%;opacity: 28%;font-weight: 700;color: green; font-family: 'Pridi', serif;\">Approved</h1>\r\n\r\n\r\n    <section class=\"content-container h-100 w-100 position-relative p-2\" style=\"max-height: 297mm !important\">\r\n      <header>\r\n        <section class=\"container p-0 m-0\">\r\n          <div class=\"row\">\r\n\r\n            <!--\t\t\t\tlogo container-->\r\n            <div class=\"col-12 d-flex title-header green position-absolute\" style=\"justify-content: space-around;  text-align: left;\">\r\n\r\n              <img src=\"../../../public/logos/county_logo_plain.png\" class=\"logo\">\r\n              <div class=\"title\" style=\"margin-top: 2rem;\">\r\n                <h1 class=\"uppercase\">County Government Of HOMABAY</h1>\r\n                <h3 class=\"uppercase\">SINGLE BUSINESS PERMIT</h3>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-8\">\r\n\r\n            </div>\r\n\r\n            <hr class=\"mb-0\">\r\n            <!--\t\t\t\tcontrol number and issue date container-->\r\n            <div class=\"col-12 validations-container\">\r\n              <div class=\"d-flex col-6\" style=\"margin-top: 0px !important\">\r\n                <table class=\"table table-bordered\">\r\n                  <tr>\r\n                    <td>Effective Date</td>\r\n                    <td style=\"font-weight: 400; font-family: 'Pridi', serif\">{{permit.startDate}} </td>\r\n                  </tr>\r\n                  <tr>\r\n                    <td>Expiry Date</td>\r\n                    <td style=\"font-weight: 400; font-family: 'Pridi', serif\">{{permit.endDate}}</td>\r\n                  </tr>\r\n                  <tr>\r\n                    <td>Duration</td>\r\n                    <td style=\"font-weight: 400; font-family: 'Pridi', serif\">{{permit.duration}}</td>\r\n                  </tr>\r\n                </table>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </section>\r\n      </header>\r\n      <section class=\"container p-3 m-0\">\r\n        <div class=\"row\">\r\n          <div class=\"col-12 \">\r\n\r\n            <p class=\"text-left\"><span class=\" green\">\r\n             County Government Of HOMABAY grants this business permit to</span>\r\n            </p>\r\n          </div>\r\n          <div class=\"col-10 pr-0\">\r\n            <table class=\"table table-bordered mb-0\">\r\n              <tbody>\r\n              <!--the name to the busibness-->\r\n              <tr>\r\n                <td>\r\n                  <p class=\"p-0 m-0 green\">Applicant/Business/Commercial Name</p>\r\n                  <p class=\"text-uppercase libre-bold m-0 p-0 title-font\">{{permit.businessName}}</p>\r\n                </td>\r\n              </tr>\r\n\r\n              <!--the KRA pin-->\r\n              <tr>\r\n                <td><span class=\"green\">KRA Pin: </span><span class=\"libre-bold\">{{permit.pinNumber}}</span></td>\r\n              </tr>\r\n\r\n              <tr>\r\n                <td>\r\n                  <span class=\"green\">Business Owner(s)</span>\r\n                  <p class=\"text-uppercase libre-bold m-0 p-0 title-font\" style=\"font-family:'Pridi', serif !important;\">{{permit.telephone1}}</p>\r\n                </td>\r\n\r\n              </tr>\r\n\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n          <div class=\"col-2 mb-1 pl-0\">\r\n            <div class=\"locence-no max-height d-flex h-100 justify-content-center align-items-center align-content-center p-2 flex-column\">\r\n              <p class=\"serial-init green\">Business ID</p>\r\n              <p> <span class=\"serial-init font-22\"><span class=\"d-none\">BID</span> <span class=\"serial title-font\" style=\"font-family: 'Pridi', serif !important; font-weight: 600;\">{{permit.businessID}}</span></span>\r\n              </p>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-10 mt-4 pr-0\">\r\n            <table class=\"table table-bordered \">\r\n              <tbody>\r\n              <tr>\r\n                <td colspan=\"2\">\r\n                  <p class=\"text-uppercase p-0 m-0 green\">To engage in the activity/Business/Profession or Occupation of</p>\r\n                  <p class=\"libre-bold m-0 p-0 serial m-0\" style=\"font-family: 'Pridi', serif !important;\">{{permit.businessActivity}} </p>\r\n                </td>\r\n              </tr>\r\n\r\n              <tr>\r\n                <td class=\"\" style=\"font-family: 'Pridi', serif; font-weight: 400;\">{{permit.businessActivityDescription}}</td>\r\n              </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n          <div class=\"col-2 mt-4 pl-0\">\r\n            <div class=\"activity-code-bg max-height d-flex h-100 justify-content-center align-items-center align-content-center p-2 flex-column\">\r\n              <p class=\"serial-init green\">Activity Code</p>\r\n              <p> <span class=\"serial-init font-22\">\r\n                <span class=\"title-font\" style=\"font-family: 'Pridi', serif; font-weight: 400; font-weight: 600;\">20/456</span>\r\n              </span></p>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"col-12 mt-4\">\r\n            <table class=\"table table-bordered \">\r\n              <tbody>\r\n              <tr>\r\n                <td class=\"text-uppercase green\">Having paid a business permit fee of</td>\r\n                <td class=\"title-font\" style=\"letter-spacing: 2px; font-family: 'Pridi', serif;\"> KES {{permit.receiptAmount}}</td>\r\n              </tr>\r\n              <tr>\r\n                <td class=\"other-num serial-init\" colspan=\"2\"><span class=\"green\">Amount in words </span> <span style=\"font-family: 'Pridi', serif; font-weight: 400;\"> *** Two thousand two hundred ***</span></td>\r\n              </tr>\r\n\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n\r\n\r\n          <div class=\"col-12 mt-4\">\r\n            <table class=\"table table-bordered\">\r\n              <tbody>\r\n\r\n              <tr class=\"d-none\">\r\n                <td colspan=\"3\" class=\"green\">Business under this permit shall be conducted at the address as indicated below</td>\r\n              </tr>\r\n              <tr>\r\n                <td colspan=\"2\"><span class=\"green\">P.O. Box : </span><span class=\"libre-bold\">{{permit.postalCode}}</span></td>\r\n                <td>Plot No. : <span class=\"libre-bold title-font\">{{permit.plotNo}}</span></td>\r\n              </tr>\r\n              <tr>\r\n                <td colspan=\"3\"><span class=\"green\">Road/Street : </span><span class=\"title-font\" style=\"font-weight:400;font-family:'Pridi', serif;\">Rongo Road</span></td>\r\n              </tr>\r\n              <tr>\r\n                <td><span class=\"green\">Building : </span><span class=\"libre-bold\">The Tower</span></td>\r\n                <td><span class=\"green\">Floor : </span><span class=\"libre-bold\">124</span></td>\r\n                <td><span class=\"green\">Door/Stall No. : </span><span class=\"libre-bold\">123</span></td>\r\n              </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n          <div class=\"col-12 mt-4\">\r\n            <div class=\"row m-0\">\r\n              <div class=\"col-6 p-0\">\r\n                <div>\r\n                  <div class=\"p-2 issue-date\">\r\n                    <span class=\"d-none\">License Covers For:</span>\r\n                    <hr class=\"py-0 my-0 d-none\">\r\n                    <p class=\"d-none\">Fire License</p>\r\n                    <hr class=\"py-0 my-0 d-none\">\r\n                    <p class=\"d-none\">Food Hygiene License</p>\r\n                    <hr class=\"py-0 my-0 d-none\">\r\n                    <p class=\"d-none\">Advertisement License</p>\r\n                  </div>\r\n                </div>\r\n\r\n                <div>\r\n                  <div class=\"p-2 issue-date\" style=\"border-top: 0px !important;\">\r\n                    <span class=\"green\">Issuing officer: </span><span class=\"\" style=\"font-weight:400;font-family:'Pridi', serif;\">Ann Awiti Oluoch</span>\r\n                    <br>\r\n                    <span class=\"green\">Designation: </span><span class=\"\" style=\"font-weight:400;font-family:'Pridi', serif;\">Director Revenue</span>\r\n                    <span class=\"green d-none\">On behalf of the Finance Department</span>\r\n                  </div>\r\n                </div>\r\n\r\n\r\n                <div>\r\n                  <div class=\"p-2 issue-date\" style=\"border-top: 0px !important;\">\r\n\r\n                    <span class=\"green\">Date of Issue: </span><br><span style=\"font-weight:400;font-family:'Pridi', serif;\">{{permit.dateCreated}}</span>\r\n                  </div>\r\n                </div>\r\n\r\n                <div class=\"w-100 the-seal-container\">\r\n                  <img :src=\"'./assets/seal.png'\" class=\"the-seal\">\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"col-6 p-0\">\r\n                <div class=\"new-qr-container p-2\">\r\n\r\n\r\n                  <p class=\"green\">By order of</p>\r\n                  <div class=\"stamp-qr\">\r\n\r\n                    <img :src=\"'./assets/sign.png'\" alt=\" The signature \" class=\"the-stamp \">\r\n                    <img class=\"mb-2 float-left theqr \" :src=\"link\" >\r\n                  </div>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"col-12\" style=\"font-size: 12px; padding: 0px; margin: 0px;\">\r\n                <p class=\"red font-14 \" style=\"font-weight: 500;\">\r\n                  Notice: Granting this permit <strong>DOES NOT EXEMPT</strong> the business identified above from complying with current regulations on Health and safety as established by the Government of Kenya and the\r\n                  <strong style=\"text-transform: uppercase;\">County Government Of HOMABAY</strong>\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n\r\n        </div>\r\n      </section>\r\n\r\n      <img :src=\"'./assets/nbk_logo.svg'\" alNBK Logo class=\"nbk\" style=\"bottom: auto; display: none;\">\r\n\r\n\r\n    </section>\r\n\r\n  </page>\r\n  <button class=\"print-btn \" onclick=\"window.print() \"><img :src=\"'./assets/printer.svg' \" alt=\"Printer Icon \"></button>\r\n  <button class=\"print-btn email-doc\" title=\"send On Email\" @click=\"mailTradePermit\"><img :src=\"'./assets/email-icon.png'\" alt=\"Printer Icon \"></button>\r\n\r\n  </body>\r\n\r\n  </html>\r\n</template>\r\n\r\n<script>\r\nimport {execute, trade} from \"@/api\";\r\n\r\n\r\nexport default {\r\n  name: \"TradePermit\",\r\n  data() {\r\n    return {\r\n      permit: {\r\n        id: null,\r\n        billNo: null,\r\n        businessID: null,\r\n        duration:null,\r\n        billTotal: null,\r\n        receiptAmount: null,\r\n        billBalance: null,\r\n        startDate: null,\r\n        endDate:null,\r\n        status:null,\r\n        printable: null,\r\n        brimsCode: null,\r\n        businessName: null,\r\n        certificateOfIncorporation: null,\r\n        pinNumber: null,\r\n        VATNumber: null,\r\n        plotNo: null,\r\n        physicalAddress: null,\r\n        businessActivity: null,\r\n        businessActivityDescription: null,\r\n        telephone1: null,\r\n        telephone2: null,\r\n        address: null,\r\n        postalCode: null,\r\n        town: null,\r\n        countyID: null,\r\n        subCountyID: null,\r\n        wardID: null,\r\n        dateCreated: null,\r\n        createdBy: null,\r\n        dateModified: null,\r\n        modifiedBy: null,\r\n        isActive: null,\r\n        latitude: null,\r\n        longitude: null\r\n      },\r\n      link:''\r\n    };\r\n  },\r\n  mounted() {\r\n    let params = {}\r\n    try {\r\n\r\n      params = JSON.parse(localStorage['params'])\r\n      console.log('####'+ params.businessID)\r\n      this.printTradePermit(params.businessID);\r\n    } catch (error) {\r\n      // ignore\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    showDate() {\r\n      const dateObj = new Date();\r\n      const currentDate =\r\n          dateObj.getDate() +\r\n          \"/\" +\r\n          dateObj.getMonth() +\r\n          \"/\" +\r\n          dateObj.getFullYear();\r\n      return currentDate;\r\n    },\r\n    formatPrice(value) {\r\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\r\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\r\n    },\r\n    printTradePermit(businessID) {\r\n      const data = new FormData();\r\n      data.append(\"function\", \"printTradePermit\");\r\n      data.append(\"businessID\", businessID);\r\n\r\n      execute(data,trade)\r\n          .then((res) => {\r\n            if (res.data.success) {\r\n              this.permit = res.data.data.permit;\r\n              this.link = \"https://chart.googleapis.com/chart?chs=300x300&cht=qr&chl=Business ID: \"+this.permit.businessID+\"\\n \"\r\n                  +this.permit.businessName+\" start date: \"+ this.permit.startDate+\" Expiry:\"+this.permit.endDate;\r\n            } else {\r\n              alert(res.data.data.message);\r\n            }\r\n          })\r\n          .catch((e) => {\r\n            alert(e.message);\r\n          });\r\n    },\r\n    mailTradePermit(){\r\n      const data = new FormData();\r\n      data.append(\"function\", \"mailTradePermit\");\r\n      data.append(\"businessID\", this.$route.params['data']);\r\n\r\n      execute(data,trade)\r\n          .then((res) => {\r\n            alert(res.data.message);\r\n          })\r\n          .catch((e) => {\r\n            alert(e.message);\r\n          });\r\n\r\n    }\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"css\" scoped>\r\n.print-btn {\r\n  display: flex;\r\n  z-index: 1000000000;\r\n  position: fixed;\r\n  background: #215939;\r\n  color: white;\r\n  top: 30px;\r\n  right: 30px;\r\n  border-radius: 50%;\r\n  padding: 1rem;\r\n  margin: 0px;\r\n  border: none;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  box-shadow: 1px 0px 20px 4px rgb(136 136 136 / 65%);\r\n  cursor: pointer;\r\n  transition: 0.4s;\r\n}\r\n\r\n.email-doc{\r\n            top: 130px;\r\n            background: #146fc5;\r\n        }\r\n\r\n.print-btn img {\r\n  height: 35px;\r\n}\r\n\r\n* {\r\n  box-sizing: border-box;\r\n  font-family: 'Montserrat', sans-serif;\r\n  -webkit-print-color-adjust: exact !important;\r\n  /* Chrome, Safari */\r\n  color-adjust: exact !important;\r\n  /*Firefox*/\r\n}\r\n\r\n:root {\r\n  --blue: #007bff;\r\n  --indigo: #6610f2;\r\n  --purple: #6f42c1;\r\n  --pink: #e83e8c;\r\n  --red: #dc3545;\r\n  --orange: #fd7e14;\r\n  --yellow: #ffc10