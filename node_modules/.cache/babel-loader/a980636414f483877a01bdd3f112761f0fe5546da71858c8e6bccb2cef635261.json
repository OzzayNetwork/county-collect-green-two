{"ast":null,"code":"import { execute, trade } from \"@/api\";\nexport default {\n  name: \"TradePermit\",\n  data() {\n    return {\n      permit: {\n        id: null,\n        billNo: null,\n        businessID: null,\n        duration: null,\n        billTotal: null,\n        receiptAmount: null,\n        billBalance: null,\n        startDate: null,\n        endDate: null,\n        status: null,\n        printable: null,\n        brimsCode: null,\n        businessName: null,\n        certificateOfIncorporation: null,\n        pinNumber: null,\n        VATNumber: null,\n        plotNo: null,\n        physicalAddress: null,\n        businessActivity: null,\n        businessActivityDescription: null,\n        telephone1: null,\n        telephone2: null,\n        address: null,\n        postalCode: null,\n        town: null,\n        countyID: null,\n        subCountyID: null,\n        wardID: null,\n        dateCreated: null,\n        createdBy: null,\n        dateModified: null,\n        modifiedBy: null,\n        isActive: null,\n        latitude: null,\n        longitude: null\n      },\n      link: ''\n    };\n  },\n  mounted() {\n    let params = {};\n    try {\n      params = JSON.parse(localStorage['params']);\n      console.log('####' + params.businessID);\n      this.printTradePermit(params.businessID);\n    } catch (error) {\n      // ignore\n    }\n  },\n  methods: {\n    showDate() {\n      const dateObj = new Date();\n      const currentDate = dateObj.getDate() + \"/\" + dateObj.getMonth() + \"/\" + dateObj.getFullYear();\n      return currentDate;\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    printTradePermit(businessID) {\n      const data = new FormData();\n      data.append(\"function\", \"printTradePermit\");\n      data.append(\"businessID\", businessID);\n      execute(data, trade).then(res => {\n        if (res.data.success) {\n          this.permit = res.data.data.permit;\n          this.link = \"https://chart.googleapis.com/chart?chs=300x300&cht=qr&chl=Business ID: \" + this.permit.businessID + \"\\n \" + this.permit.businessName + \" start date: \" + this.permit.startDate + \" Expiry:\" + this.permit.endDate;\n        } else {\n          alert(res.data.data.message);\n        }\n      }).catch(e => {\n        alert(e.message);\n      });\n    },\n    mailTradePermit() {\n      const data = new FormData();\n      data.append(\"function\", \"mailTradePermit\");\n      data.append(\"businessID\", this.$route.params['data']);\n      execute(data, trade).then(res => {\n        alert(res.data.message);\n      }).catch(e => {\n        alert(e.message);\n      });\n    }\n  }\n};","map":{"version":3,"names":["execute","trade","name","data","permit","id","billNo","businessID","duration","billTotal","receiptAmount","billBalance","startDate","endDate","status","printable","brimsCode","businessName","certificateOfIncorporation","pinNumber","VATNumber","plotNo","physicalAddress","businessActivity","businessActivityDescription","telephone1","telephone2","address","postalCode","town","countyID","subCountyID","wardID","dateCreated","createdBy","dateModified","modifiedBy","isActive","latitude","longitude","link","mounted","params","JSON","parse","localStorage","console","log","printTradePermit","error","methods","showDate","dateObj","Date","currentDate","getDate","getMonth","getFullYear","formatPrice","value","val","toFixed","replace","toString","FormData","append","then","res","success","alert","message","catch","e","mailTradePermit","$route"],"sources":["/Applications/XAMPP/xamppfiles/htdocs/homabaytest/county-biller/public/PrintDocs/Trade-Permit/TradePermit.vue"],"sourcesContent":["<template>\n  <html>\n\n  <head>\n    <meta charset=\"utf-8\">\n    <title class=\"\">HOMABAY County Trade Permit</title>\n    <link href=\"https://fonts.googleapis.com/css?family=Aldrich|Fira+Sans:200,300,400,500,700,800,900|Norican&display=swap&subset=cyrillic\" rel=\"stylesheet\">\n    <link href=\"https://fonts.googleapis.com/css?family=Libre+Baskerville:400,400i,700|Open+Sans:300,400,600,700&display=swap\" rel=\"stylesheet\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\">\n    <link href=\"https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,600;0,700;0,800;1,300;1,400;1,600;1,700;1,800&display=swap\" rel=\"stylesheet\">\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n    <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n    <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n    <link href=\"https://fonts.googleapis.com/css2?family=Pridi:wght@200;300;400;500;600;700&display=swap\" rel=\"stylesheet\">\n\n  </head>\n\n  <body>\n  <page size=\"A4\" class=\"p-2 container w-100 h-100\">\n\n    <h1 style=\"text-transform: uppercase;font-size: 137px;position: absolute;transform: rotate(45deg);top: 40%;opacity: 28%;font-weight: 700;color: green; font-family: 'Pridi', serif;\">Approved</h1>\n\n\n    <section class=\"content-container h-100 w-100 position-relative p-2\" style=\"max-height: 297mm !important\">\n      <header>\n        <section class=\"container p-0 m-0\">\n          <div class=\"row\">\n\n            <!--\t\t\t\tlogo container-->\n            <div class=\"col-12 d-flex title-header green position-absolute\" style=\"justify-content: space-around;  text-align: left;\">\n\n              <img src=\"../../../public/logos/county_logo_plain.png\" class=\"logo\">\n              <div class=\"title\" style=\"margin-top: 2rem;\">\n                <h1 class=\"uppercase\">County Government Of HOMABAY</h1>\n                <h3 class=\"uppercase\">SINGLE BUSINESS PERMIT</h3>\n              </div>\n            </div>\n            <div class=\"col-8\">\n\n            </div>\n\n            <hr class=\"mb-0\">\n            <!--\t\t\t\tcontrol number and issue date container-->\n            <div class=\"col-12 validations-container\">\n              <div class=\"d-flex col-6\" style=\"margin-top: 0px !important\">\n                <table class=\"table table-bordered\">\n                  <tr>\n                    <td>Effective Date</td>\n                    <td style=\"font-weight: 400; font-family: 'Pridi', serif\">{{permit.startDate}} </td>\n                  </tr>\n                  <tr>\n                    <td>Expiry Date</td>\n                    <td style=\"font-weight: 400; font-family: 'Pridi', serif\">{{permit.endDate}}</td>\n                  </tr>\n                  <tr>\n                    <td>Duration</td>\n                    <td style=\"font-weight: 400; font-family: 'Pridi', serif\">{{permit.duration}}</td>\n                  </tr>\n                </table>\n              </div>\n            </div>\n          </div>\n        </section>\n      </header>\n      <section class=\"container p-3 m-0\">\n        <div class=\"row\">\n          <div class=\"col-12 \">\n\n            <p class=\"text-left\"><span class=\" green\">\n             County Government Of HOMABAY grants this business permit to</span>\n            </p>\n          </div>\n          <div class=\"col-10 pr-0\">\n            <table class=\"table table-bordered mb-0\">\n              <tbody>\n              <!--the name to the busibness-->\n              <tr>\n                <td>\n                  <p class=\"p-0 m-0 green\">Applicant/Business/Commercial Name</p>\n                  <p class=\"text-uppercase libre-bold m-0 p-0 title-font\">{{permit.businessName}}</p>\n                </td>\n              </tr>\n\n              <!--the KRA pin-->\n              <tr>\n                <td><span class=\"green\">KRA Pin: </span><span class=\"libre-bold\">{{permit.pinNumber}}</span></td>\n              </tr>\n\n              <tr>\n                <td>\n                  <span class=\"green\">Business Owner(s)</span>\n                  <p class=\"text-uppercase libre-bold m-0 p-0 title-font\" style=\"font-family:'Pridi', serif !important;\">{{permit.telephone1}}</p>\n                </td>\n\n              </tr>\n\n              </tbody>\n            </table>\n          </div>\n          <div class=\"col-2 mb-1 pl-0\">\n            <div class=\"locence-no max-height d-flex h-100 justify-content-center align-items-center align-content-center p-2 flex-column\">\n              <p class=\"serial-init green\">Business ID</p>\n              <p> <span class=\"serial-init font-22\"><span class=\"d-none\">BID</span> <span class=\"serial title-font\" style=\"font-family: 'Pridi', serif !important; font-weight: 600;\">{{permit.businessID}}</span></span>\n              </p>\n            </div>\n          </div>\n          <div class=\"col-10 mt-4 pr-0\">\n            <table class=\"table table-bordered \">\n              <tbody>\n              <tr>\n                <td colspan=\"2\">\n                  <p class=\"text-uppercase p-0 m-0 green\">To engage in the activity/Business/Profession or Occupation of</p>\n                  <p class=\"libre-bold m-0 p-0 serial m-0\" style=\"font-family: 'Pridi', serif !important;\">{{permit.businessActivity}} </p>\n                </td>\n              </tr>\n\n              <tr>\n                <td class=\"\" style=\"font-family: 'Pridi', serif; font-weight: 400;\">{{permit.businessActivityDescription}}</td>\n              </tr>\n              </tbody>\n            </table>\n          </div>\n          <div class=\"col-2 mt-4 pl-0\">\n            <div class=\"activity-code-bg max-height d-flex h-100 justify-content-center align-items-center align-content-center p-2 flex-column\">\n              <p class=\"serial-init green\">Activity Code</p>\n              <p> <span class=\"serial-init font-22\">\n                <span class=\"title-font\" style=\"font-family: 'Pridi', serif; font-weight: 400; font-weight: 600;\">20/456</span>\n              </span></p>\n            </div>\n          </div>\n\n          <div class=\"col-12 mt-4\">\n            <table class=\"table table-bordered \">\n              <tbody>\n              <tr>\n                <td class=\"text-uppercase green\">Having paid a business permit fee of</td>\n                <td class=\"title-font\" style=\"letter-spacing: 2px; font-family: 'Pridi', serif;\"> KES {{permit.receiptAmount}}</td>\n              </tr>\n              <tr>\n                <td class=\"other-num serial-init\" colspan=\"2\"><span class=\"green\">Amount in words </span> <span style=\"font-family: 'Pridi', serif; font-weight: 400;\"> *** Two thousand two hundred ***</span></td>\n              </tr>\n\n              </tbody>\n            </table>\n          </div>\n\n\n          <div class=\"col-12 mt-4\">\n            <table class=\"table table-bordered\">\n              <tbody>\n\n              <tr class=\"d-none\">\n                <td colspan=\"3\" class=\"green\">Business under this permit shall be conducted at the address as indicated below</td>\n              </tr>\n              <tr>\n                <td colspan=\"2\"><span class=\"green\">P.O. Box : </span><span class=\"libre-bold\">{{permit.postalCode}}</span></td>\n                <td>Plot No. : <span class=\"libre-bold title-font\">{{permit.plotNo}}</span></td>\n              </tr>\n              <tr>\n                <td colspan=\"3\"><span class=\"green\">Road/Street : </span><span class=\"title-font\" style=\"font-weight:400;font-family:'Pridi', serif;\">Rongo Road</span></td>\n              </tr>\n              <tr>\n                <td><span class=\"green\">Building : </span><span class=\"libre-bold\">The Tower</span></td>\n                <td><span class=\"green\">Floor : </span><span class=\"libre-bold\">124</span></td>\n                <td><span class=\"green\">Door/Stall No. : </span><span class=\"libre-bold\">123</span></td>\n              </tr>\n              </tbody>\n            </table>\n          </div>\n          <div class=\"col-12 mt-4\">\n            <div class=\"row m-0\">\n              <div class=\"col-6 p-0\">\n                <div>\n                  <div class=\"p-2 issue-date\">\n                    <span class=\"d-none\">License Covers For:</span>\n                    <hr class=\"py-0 my-0 d-none\">\n                    <p class=\"d-none\">Fire License</p>\n                    <hr class=\"py-0 my-0 d-none\">\n                    <p class=\"d-none\">Food Hygiene License</p>\n                    <hr class=\"py-0 my-0 d-none\">\n                    <p class=\"d-none\">Advertisement License</p>\n                  </div>\n                </div>\n\n                <div>\n                  <div class=\"p-2 issue-date\" style=\"border-top: 0px !important;\">\n                    <span class=\"green\">Issuing officer: </span><span class=\"\" style=\"font-weight:400;font-family:'Pridi', serif;\">Ann Awiti Oluoch</span>\n                    <br>\n                    <span class=\"green\">Designation: </span><span class=\"\" style=\"font-weight:400;font-family:'Pridi', serif;\">Director Revenue</span>\n                    <span class=\"green d-none\">On behalf of the Finance Department</span>\n                  </div>\n                </div>\n\n\n                <div>\n                  <div class=\"p-2 issue-date\" style=\"border-top: 0px !important;\">\n\n                    <span class=\"green\">Date of Issue: </span><br><span style=\"font-weight:400;font-family:'Pridi', serif;\">{{permit.dateCreated}}</span>\n                  </div>\n                </div>\n\n                <div class=\"w-100 the-seal-container\">\n                  <img :src=\"'./assets/seal.png'\" class=\"the-seal\">\n                </div>\n              </div>\n\n              <div class=\"col-6 p-0\">\n                <div class=\"new-qr-container p-2\">\n\n\n                  <p class=\"green\">By order of</p>\n                  <div class=\"stamp-qr\">\n\n                    <img :src=\"'./assets/sign.png'\" alt=\" The signature \" class=\"the-stamp \">\n                    <img class=\"mb-2 float-left theqr \" :src=\"link\" >\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"col-12\" style=\"font-size: 12px; padding: 0px; margin: 0px;\">\n                <p class=\"red font-14 \" style=\"font-weight: 500;\">\n                  Notice: Granting this permit <strong>DOES NOT EXEMPT</strong> the business identified above from complying with current regulations on Health and safety as established by the Government of Kenya and the\n                  <strong style=\"text-transform: uppercase;\">County Government Of HOMABAY</strong>\n                </p>\n              </div>\n            </div>\n          </div>\n\n\n        </div>\n      </section>\n\n      <img :src=\"'./assets/nbk_logo.svg'\" alNBK Logo class=\"nbk\" style=\"bottom: auto; display: none;\">\n\n\n    </section>\n\n  </page>\n  <button class=\"print-btn \" onclick=\"window.print() \"><img :src=\"'./assets/printer.svg' \" alt=\"Printer Icon \"></button>\n  <button class=\"print-btn email-doc\" title=\"send On Email\" @click=\"mailTradePermit\"><img :src=\"'./assets/email-icon.png'\" alt=\"Printer Icon \"></button>\n\n  </body>\n\n  </html>\n</template>\n\n<script>\nimport {execute, trade} from \"@/api\";\n\n\nexport default {\n  name: \"TradePermit\",\n  data() {\n    return {\n      permit: {\n        id: null,\n        billNo: null,\n        businessID: null,\n        duration:null,\n        billTotal: null,\n        receiptAmount: null,\n        billBalance: null,\n        startDate: null,\n        endDate:null,\n        status:null,\n        printable: null,\n        brimsCode: null,\n        businessName: null,\n        certificateOfIncorporation: null,\n        pinNumber: null,\n        VATNumber: null,\n        plotNo: null,\n        physicalAddress: null,\n        businessActivity: null,\n        businessActivityDescription: null,\n        telephone1: null,\n        telephone2: null,\n        address: null,\n        postalCode: null,\n        town: null,\n        countyID: null,\n        subCountyID: null,\n        wardID: null,\n        dateCreated: null,\n        createdBy: null,\n        dateModified: null,\n        modifiedBy: null,\n        isActive: null,\n        latitude: null,\n        longitude: null\n      },\n      link:''\n    };\n  },\n  mounted() {\n    let params = {}\n    try {\n\n      params = JSON.parse(localStorage['params'])\n      console.log('####'+ params.businessID)\n      this.printTradePermit(params.businessID);\n    } catch (error) {\n      // ignore\n    }\n\n  },\n  methods: {\n    showDate() {\n      const dateObj = new Date();\n      const currentDate =\n          dateObj.getDate() +\n          \"/\" +\n          dateObj.getMonth() +\n          \"/\" +\n          dateObj.getFullYear();\n      return currentDate;\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    printTradePermit(businessID) {\n      const data = new FormData();\n      data.append(\"function\", \"printTradePermit\");\n      data.append(\"businessID\", businessID);\n\n      execute(data,trade)\n          .then((res) => {\n            if (res.data.success) {\n              this.permit = res.data.data.permit;\n              this.link = \"https://chart.googleapis.com/chart?chs=300x300&cht=qr&chl=Business ID: \"+this.permit.businessID+\"\\n \"\n                  +this.permit.businessName+\" start date: \"+ this.permit.startDate+\" Expiry:\"+this.permit.endDate;\n            } else {\n              alert(res.data.data.message);\n            }\n          })\n          .catch((e) => {\n            alert(e.message);\n          });\n    },\n    mailTradePermit(){\n      const data = new FormData();\n      data.append(\"function\", \"mailTradePermit\");\n      data.append(\"businessID\", this.$route.params['data']);\n\n      execute(data,trade)\n          .then((res) => {\n            alert(res.data.message);\n          })\n          .catch((e) => {\n            alert(e.message);\n          });\n\n    }\n\n  },\n}\n</script>\n\n<style lang=\"css\" scoped>\n.print-btn {\n  display: flex;\n  z-index: 1000000000;\n  position: fixed;\n  background: #215939;\n  color: white;\n  top: 30px;\n  right: 30px;\n  border-radius: 50%;\n  padding: 1rem;\n  margin: 0px;\n  border: none;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  box-shadow: 1px 0px 20px 4px rgb(136 136 136 / 65%);\n  cursor: pointer;\n  transition: 0.4s;\n}\n\n.email-doc{\n            top: 130px;\n            background: #146fc5;\n        }\n\n.print-btn img {\n  height: 35px;\n}\n\n* {\n  box-sizing: border-box;\n  font-family: 'Montserrat', sans-serif;\n  -webkit-print-color-adjust: exact !important;\n  /* Chrome, Safari */\n  color-adjust: exact !important;\n  /*Firefox*/\n}\n\n:root {\n  --blue: #007bff;\n  --indigo: #6610f2;\n  --purple: #6f42c1;\n  --pink: #e83e8c;\n  --red: #dc3545;\n  --orange: #fd7e14;\n  --yellow: #ffc107;\n  --green: #146fc5;\n  --teal: #20c997;\n  --cyan: #17a2b8;\n  --white: #fff;\n  --gray: #6c757d;\n  --gray-dark: #343a40;\n  --primary: #007bff;\n  --secondary: #6c757d;\n  --success: #146fc5;\n  --info: #17a2b8;\n  --warning: #ffc107;\n  --danger: #dc3545;\n  --light: #f8f9fa;\n  --dark: #343a40;\n  --breakpoint-xs: 0;\n  --breakpoint-sm: 576px;\n  --breakpoint-md: 768px;\n  --breakpoint-lg: 992px;\n  --breakpoint-xl: 1200px;\n  --font-family-sans-serif: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\";\n  --font-family-monospace: SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace;\n}\n\n.w-100 {\n  width: 100%!im