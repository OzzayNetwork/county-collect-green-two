{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { biller, execute } from \"@/api\";\nimport NavigationBar from \"@/components/Navigation\";\nimport router from \"@/router\";\nexport default {\n  name: \"LoginPage\",\n  components: {\n    NavigationBar\n  },\n  data() {\n    return {\n      message: '',\n      alert: {\n        success: false,\n        info: false,\n        failed: false\n      },\n      billDetails: [],\n      billInfo: [],\n      receiptInfos: [],\n      billDetail: {\n        id: \"179\",\n        billNo: \"BLL0411031700\",\n        incomeTypeID: \"2,\",\n        incomeTypeDescription: \"Informal Sector Fees,\",\n        costCenterNo: \"\",\n        accountNo: \",\",\n        feeID: \"18,\",\n        detailAmount: \"1\",\n        receiptAmount: \"1\",\n        billBalance: \"0\",\n        wardID: \"472751373\",\n        subCountyID: \"47275\",\n        status: \"PAID\",\n        dateCreated: \"2023-04-11 12:17:00.192127\"\n      },\n      billInfoTotal: 0,\n      billNo: ''\n    };\n  },\n  computed: {\n    totalReceiptAmount() {\n      return this.formatPrice(this.billDetails.reduce((sum, billDetail) => sum + parseInt(billDetail.receiptAmount), 0));\n    },\n    totalBillBalance() {\n      return this.formatPrice(this.billDetails.reduce((sum, billDetail) => sum + parseInt(billDetail.billBalance), 0));\n    },\n    totalDetailAmount() {\n      return this.formatPrice(this.billDetails.reduce((sum, billDetail) => sum + parseInt(billDetail.detailAmount), 0));\n    }\n  },\n  mounted() {\n    this.getBills();\n  },\n  methods: {\n    formatPrice(value) {\n      var formatter = new Intl.NumberFormat('en-US', {\n        style: 'currency',\n        currency: 'KES',\n        minimumFractionDigits: 2\n      });\n      return formatter.format(value);\n    },\n    gotTo(route) {\n      router.push(route);\n    },\n    getBills() {\n      const data = new FormData();\n      data.append(\"function\", \"getBills\");\n      data.append(\"status\", \"UNPAID\");\n      execute(data, biller).then(res => {\n        if (res.data.success) {\n          this.billDetails = res.data.data.billDetails;\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    },\n    getBill(billNo) {\n      this.billNo = billNo;\n      const data = new FormData();\n      data.append(\"function\", \"getBill\");\n      data.append(\"billNo\", billNo);\n      execute(data, biller).then(res => {\n        if (res.data.success) {\n          this.billInfo = res.data.data.billInfo;\n          this.billDetail = res.data.data.billDetails;\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    },\n    printBill() {\n      localStorage['params'] = JSON.stringify({\n        billNo: this.billDetail.billNo\n      });\n      const routeData = this.$router.resolve({\n        name: 'bill'\n      });\n      window.open(routeData.href, '_blank');\n    }\n  }\n};","map":{"version":3,"names":["biller","execute","NavigationBar","router","name","components","data","message","alert","success","info","failed","billDetails","billInfo","receiptInfos","billDetail","id","billNo","incomeTypeID","incomeTypeDescription","costCenterNo","accountNo","feeID","detailAmount","receiptAmount","billBalance","wardID","subCountyID","status","dateCreated","billInfoTotal","computed","totalReceiptAmount","formatPrice","reduce","sum","parseInt","totalBillBalance","totalDetailAmount","mounted","getBills","methods","value","formatter","Intl","NumberFormat","style","currency","minimumFractionDigits","format","gotTo","route","push","FormData","append","then","res","catch","e","getBill","printBill","localStorage","JSON","stringify","routeData","$router","resolve","window","open","href"],"sources":["/Applications/XAMPP/xamppfiles/htdocs/homabaytest/county-biller/src/components/UnPaidBills.vue"],"sourcesContent":["<template>\n    <div>\n\n\n        <head>\n\n            <!-- App favicon -->\n            <link rel=\"shortcut icon\" href=\"assets/images/favicon.png\">\n            <!-- droping file -->\n            <link href=\"assets/libs/dropzone/min/dropzone.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n\n            <!-- DataTables -->\n            <link href  =\"assets/libs/datatables.net-bs4/css/dataTables.bootstrap4.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n            <link href=\"assets/libs/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n\n            <!-- Bootstrap Css -->\n            <link href=\"assets/css/bootstrap.min.css\" id=\"bootstrap-style\" rel=\"stylesheet\" type=\"text/css\" />\n            <!-- Bootstrap select styling -->\n            <link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/css/bootstrap-select.min.css\">\n            <!-- Icons Css -->\n            <link href=\"assets/css/icons.min.css\" rel=\"stylesheet\" type=\"text/css\" />\n            <!-- App Css-->\n            <link href=\"assets/css/app.min.css\" id=\"app-style\" rel=\"stylesheet\" type=\"text/css\" />\n            <!-- custom Css-->\n            <link href=\"assets/css/custom.css\" id=\"app-style\" rel=\"stylesheet\" type=\"text/css\" />\n\n        </head>\n\n        <body data-sidebar=\"dark\">\n\n        <!-- Begin page -->\n        <div id=\"layout-wrapper\">\n\n            <NavigationBar/>\n\n            <!-- ============================================================== -->\n            <!-- Start right Content here -->\n            <!-- ============================================================== -->\n            <div class=\"main-content\">\n\n                <div class=\"page-content\">\n                    <div class=\"container-fluid\">\n\n                        <!-- start page title -->\n                        <div class=\"row\">\n                            <div class=\"col-12\">\n                                <div class=\"page-title-box d-sm-flex align-items-center justify-content-between\">\n                                    <h4 class=\"mb-sm-0 font-size-18\">Bills raised</h4>\n\n                                    <div class=\"page-title-right\">\n                                        <ol class=\"breadcrumb m-0\">\n                                            <li class=\"breadcrumb-item\"><a href=\"index.html\">Dashboards</a></li>\n                                            <li class=\"breadcrumb-item\"><a href=\"#\">Bills</a></li>\n                                            <li class=\"breadcrumb-item active\">Bills Raised</li>\n                                        </ol>\n                                    </div>\n\n                                </div>\n                            </div>\n                        </div>\n                        <!-- end page title -->\n                        <div class=\"row\">\n                            <div class=\"col-12\">\n                                <div class=\"card\">\n                                    <div class=\"card-header bg-white pt-0 pr-0 p-0 d-flex justify-content-between align-items-center w-100 border-bottom\">\n\n                                        <div class=\"btn-toolbar p-3 d-flex justify-content-between align-items-center w-100\" role=\"toolbar\">\n                                            <h4 class=\"card-title text-capitalize mb-0 \">\n                                                All Bills Raised Today\n                                            </h4>\n\n                                            <div class=\"d-flex\">\n                                                <a @click=\"gotTo('new-bill')\"  type=\"button\" class=\"btn btn-primary dropdown-toggle option-selector me-3 text-capitalize\">\n                                                    <i class=\"mdi mdi-plus-circle-outline  font-size-16\"></i> <span class=\"pl-1 d-md-inline\">Create A Bill</span>\n                                                </a>\n\n                                            </div>\n\n\n                                        </div>\n                                        <div class=\"btn-toolbar p-3 align-items-center d-none animated delete-tool-bar\" role=\"toolbar\">\n                                            <button type=\"button\" class=\"btn btn-primary waves-effect btn-label waves-light me-3\"><i class=\"mdi mdi-printer label-icon\"></i> Print Selected Invoices</button>\n                                        </div>\n                                    </div>\n                                    <div class=\"card-body\">\n                                        <div class=\"table-responsive\">\n\n                                            <table class=\"table align-middle table-hover  contacts-table table-striped \" id=\"datatable-buttons\">\n                                                <thead class=\"table-light\">\n                                                <tr class=\"table-dark\">\n                                                    <th class=\"text-nowrap \" scope=\"col\">NO.</th>\n                                                    <th class=\"text-nowrap \" scope=\"col\">Bill No</th>\n                                                    <th class=\"text-nowrap \" scope=\"col\">Description</th>\n                                                    <th class=\"text-nowrap \" scope=\"col\">Sub County</th>\n                                                    <th class=\"text-nowrap \" scope=\"col\">Ward</th>\n                                                    <th class=\"text-nowrap \" scope=\"col\">Zone</th>\n                                                    <th class=\"text-nowrap \" scope=\"col\">Attendant</th>\n                                                    <th class=\"text-nowrap text-right \" scope=\"col\">Billed Amount</th>\n                                                    <th class=\"text-nowrap text-right \" scope=\"col\">Amount Paid</th>\n                                                    <th class=\"text-nowrap text-right \" scope=\"col\">Balance</th>\n                                                    <th class=\"text-nowrap \" scope=\"col\" >Status</th>\n                                                    <th></th>\n                                                </tr>\n                                                </thead>\n                                                <tbody>\n                                                <tr v-for=\"(item, index) in billDetails\" :value=\"item\" :key=\"index\">\n\n                                                    <td class=\"text-capitalize text-nowrap\">{{index+1}}</td>\n                                                    <td class=\"text-capitalize text-nowrap\">{{item.billNo}}</td>\n                                                    <td class=\"text-capitalize text-nowrap\">{{item.incomeTypeDescription}}</td>\n                                                    <td class=\"text-capitalize text-nowrap\">{{item.subCountyName}}</td>\n                                                    <td class=\"text-capitalize text-nowrap\">{{item.wardName}}</td>\n                                                    <td class=\"text-capitalize text-nowrap\">{{item.zone}}</td>\n                                                    <td class=\"text-capitalize text-nowrap\">{{item.names}}</td>\n                                                    <td class=\"text-nowrap text-right \">\n                                                        <span class=\"fw-semibold \">{{formatPrice(item.detailAmount)}}</span>\n                                                    </td>\n                                                    <td class=\"text-nowrap text-right \">\n                                                        <span class=\"fw-semibold \">{{formatPrice(item.receiptAmount)}}</span>\n                                                    </td>\n                                                    <td class=\"text-nowrap text-right \">\n                                                        <span class=\"fw-semibold \">{{formatPrice(item.billBalance)}}</span>\n                                                    </td>\n                                                    <td>\n                                                        <span v-if=\"item.status==='PAID'\" class=\"badge badge-soft-success text-uppercase\">{{item.status}}</span>\n                                                        <span v-if=\"item.status !=='PAID'\" class=\"badge badge-soft-danger text-uppercase\">UNPAID</span>\n                                                    </td>\n                                                    <td>\n                                                        <div class=\"d-flex justify-content-end\">\n                                                            <button @click=\"getBill(item.billNo)\" type=\"button\" class=\"btn btn-primary btn-sm waves-effect waves-light text-nowrap me-3\" data-toggle=\"modal\" data-target=\"#billDetail\">View Details</button>\n                                                            <div class=\"dropdown\">\n                                                                <a class=\"text-muted font-size-16\" role=\"button\" data-bs-toggle=\"dropdown\" aria-haspopup=\"true\">\n                                                                    <i class=\"bx bx-dots-vertical-rounded\"></i>\n                                                                </a>\n\n                                                                <div class=\"dropdown-menu dropdown-menu-end \">\n                                                                    <a class=\"dropdown-item \" data-bs-toggle=\"modal\" data-bs-target=\".receipting-modal\" href=\"# \"><i class=\"font-size-15 mdi mdi-eye me-3 \"></i>View</a>\n                                                                    <a class=\"dropdown-item \" href=\"# \"><i class=\"font-size-15 mdi mdi-printer me-3 \"></i>Print</a>\n                                                                    <a class=\"dropdown-item \" href=\"# \"><i class=\"font-size-15 mdi mdi-email me-3 \"></i>Send Email Reminder</a>\n                                                                    <a class=\"dropdown-item \" href=\"# \"><i class=\"font-size-15 mdi mdi-chat me-3 \"></i>Send SMS Reminder</a>\n\n                                                                </div>\n                                                            </div>\n                                                        </div>\n                                                    </td>\n                                                </tr>\n                                                </tbody>\n                                                <tfoot class=\"table-dark\">\n                                                <tr>\n                                                    <th >\n                                                        {{billDetails.length}}\n                                                    </th>\n                                                    <th></th>\n                                                    <th></th>\n                                                    <th></th>\n                                                    <th></th>\n                                                    <th></th>\n                                                    <td class=\"text-nowrap text-right \">\n                                                        <span class=\"fw-semibold \"> {{totalDetailAmount}}</span>\n                                                    </td>\n                                                    <td class=\"text-nowrap text-right \">\n                                                        <span class=\"fw-semibold \">{{totalReceiptAmount}}</span>\n                                                    </td>\n                                                    <td class=\"text-nowrap text-right \">\n                                                        <span class=\"fw-semibold \">{{totalBillBalance}}</span>\n                                                    </td>\n                                                    <th></th>\n                                                    <th></th>\n                                                </tr>\n\n                                                </tfoot>\n                                            </table>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <!-- end col -->\n                        </div>\n\n                        <!-- end row -->\n                    </div>\n                    <!-- container-fluid -->\n                </div>\n                <!-- End Page-content -->\n\n                <!-- receipting modal -->\n                <div id=\"billDetail\" class=\"modal fade receipting-modal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"myLargeModalLabel\" aria-hidden=\"true\">\n                    <div class=\"modal-dialog modal-lg modal-dialog-centered\">\n                        <div class=\"modal-content\">\n                            <div class=\"modal-header\">\n                                <h5 class=\"modal-title\" id=\"myLargeModalLabel\">Bill details</h5>\n                                <button type=\"button\" class=\"btn-close\" data-dismiss=\"modal\" aria-label=\"Close\"></button>\n                            </div>\n                            <div class=\"modal-body\">\n                                <div class=\"row\">\n                                    <div class=\"col-6\">\n                                        <address>\n                                            <strong class=\"text-capitalize\">Bill Prepared for:</strong><br> {{billDetail.description}}<br>\n                                            <br>\n                                            On {{billDetail.dateCreated}}\n\n                                        </address>\n                                    </div>\n                                    <div class=\"col-sm-6 text-sm-end d-none\">\n                                        <address class=\"mt-2 mt-sm-0\">\n                                            <strong>Payment Method</strong><br>\n                                            <span class=\"fw-medium\">MPESA</span><br>\n                                            Phone No. 0704 549 859<br> <br>\n                                            On 1 Mar 2022, 10:20 AM\n                                        </address>\n                                    </div>\n                                </div>\n                                <div class=\"col-12\">\n                                    <div class=\"py-2 mt-3\">\n                                        <h3 class=\"font-size-15 fw-bold\">{{billNo}}</h3>\n                                    </div>\n                                </div>\n                                <div class=\"col-12\">\n                                    <div class=\"table-responsive\">\n                                        <table class=\"table table-nowrap vertical-align-middle\">\n                                            <thead class=\"bg-light\">\n                                            <tr>\n                                                <th style=\"width: 70px;\">No.</th>\n                                                <th>Customer</th>\n                                                <th>Description</th>\n                                                <th class=\"text-end\">Amount</th>\n\n                                            </tr>\n                                            </thead>\n                                            <tfoot class=\"bg-light\">\n                                            <tr>\n                                                <td></td>\n                                                <td colspan=\"2\" class=\"border-0 text-end text-right\">\n                                                    <strong>Total</strong>\n                                                </td>\n                                                <td class=\"border-0 text-end\">\n                                                    <h5 class=\"m-0 text-uppercase fw-bold\">KES {{billDetail.detailAmount}}</h5>\n                                                </td>\n\n                                            </tr>\n                                            </tfoot>\n                                            <tbody>\n                                            <tr v-for=\"(item, index) in billInfo\" :value=\"item\" :key=\"index\"\n                                                    title=\"Driving Without identification plates affixed or plates not fixed in the prescribed manner\">\n                                                <td>{{index+1}}</td>\n                                                <td>\n                                                    <p class=\"the-bill-item mb-0 p-0 m-0\">{{item.customer}}</p>\n                                                </td>\n                                                <td>\n                                                    <p class=\"the-bill-item mb-0 p-0 m-0\">{{item.description}}</p>\n                                                </td>\n                                                <td class=\"text-end\">KES {{item.billTotal}}</td>\n\n                                            </tr>\n                                            </tbody>\n                                        </table>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"modal-footer\">\n\n                                <a @click=\"printBill()\" type=\"button\" class=\"btn btn-primary waves-effect waves-light\">\n                                    <i class=\"mdi mdi-printer font-size-16 align-middle me-2\"></i> Print Bill\n                                </a>\n                            </div>\n                        </div>\n                        <!-- /.modal-content -->\n                    </div>\n                    <!-- /.modal-dialog -->\n                </div>\n                <!-- end of receipting modal -->\n\n\n                <footer class=\"footer \">\n                    <div class=\"container-fluid \">\n                        <div class=\"row \">\n                            <div class=\"col-sm-6 \">\n<!--                                <script>-->\n<!--                                    document.write(new Date().getFullYear())-->\n<!--                                </script> Â© RevenueSure-->\n                            </div>\n                            <div class=\"col-sm-6 \">\n                                <div class=\"text-sm-end d-sm-block \">\n                                    Developed by County\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </footer>\n            </div>\n            <!-- end main content-->\n\n        </div>\n        <!-- END layout-wrapper -->\n\n        </body>\n\n    </div>\n</template>\n\n<script>\n    import {biller, execute} from \"@/api\";\n    import NavigationBar from \"@/components/Navigation\";\n    import router from \"@/router\";\n\n    export default  {\n        name: \"LoginPage\",\n        components: {NavigationBar},\n        data() {\n            return{\n                message:'',\n                alert:{\n                    success: false,\n                    info: false,\n                    failed:false\n                },\n                billDetails:[],\n                billInfo:[],\n                receiptInfos: [],\n                billDetail:{\n                    id: \"179\",\n                    billNo: \"BLL0411031700\",\n                    incomeTypeID: \"2,\",\n                    incomeTypeDescription: \"Informal Sector Fees,\",\n                    costCenterNo: \"\",\n                    accountNo: \",\",\n                    feeID: \"18,\",\n                    detailAmount: \"1\",\n                    receiptAmount: \"1\",\n                    billBalance: \"0\",\n                    wardID: \"472751373\",\n                    subCountyID: \"47275\",\n                    status: \"PAID\",\n                    dateCreated: \"2023-04-11 12:17:00.192127\"},\n                billInfoTotal: 0,\n                billNo:''\n            }\n        },\n        computed:{\n            totalReceiptAmount() {\n                return this.formatPrice(this.billDetails.reduce((sum, billDetail) => sum + parseInt(billDetail.receiptAmount), 0));\n            },totalBillBalance() {\n                return this.formatPrice(this.billDetails.reduce((sum, billDetail) => sum + parseInt(billDetail.billBalance), 0));\n            },\n            totalDetailAmount() {\n                return this.formatPrice(this.billDetails.reduce((sum, billDetail) => sum + parseInt(billDetail.detailAmount), 0));\n            }\n\n        },\n        mounted() {\n            this.getBills()\n        },\n        methods: {\n            formatPrice(value) {\n                var formatter = new Intl.NumberFormat('en-US', {\n                    style: 'currency',\n                    currency: 'KES',\n                    minimumFractionDigits: 2\n                });\n                return formatter.format(value);\n            },\n            gotTo(route){\n                router.push(route)\n            },\n            getBills(){\n     