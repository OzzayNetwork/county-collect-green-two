ntent\">\r\n                <div class=\"container-fluid\">\r\n\r\n                    <!-- start page title -->\r\n                    <div class=\"row\">\r\n                        <div class=\"col-12\">\r\n                            <div class=\"page-title-box d-sm-flex align-items-center justify-content-between\">\r\n                                <h4 class=\"mb-sm-0 font-size-18\">Department Configurations</h4>\r\n\r\n                                <div class=\"page-title-right\">\r\n                                    <ol class=\"breadcrumb m-0\">\r\n                                        <li class=\"breadcrumb-item\"><a @click=\"gotTo('dashboard')\">Dashboards</a></li>\r\n                                        <li class=\"breadcrumb-item active\">Zones</li>\r\n                                    </ol>\r\n                                </div>\r\n\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <!-- end page title -->\r\n\r\n                    <!-- eTransactions table -->\r\n\r\n                    <div class=\"row\">\r\n                        <div v-if=\"permission==='WRITE'\" class=\"col-6\">\r\n\r\n                            <div class=\"card p-4\">\r\n\r\n\r\n\r\n                                <div class=\"card-body\">\r\n                                    <h4 class=\"card-title text-capitalize\">Create new department</h4>\r\n                                    <p class=\"pb-4\">Fill in below to register a departments\r\n                                    </p>\r\n                                    <hr class=\"mb-5 d-none\">\r\n                                        <div class=\"row form-space\">\r\n                                            <div class=\"col-12\">\r\n                                                <div class=\"form-group\">\r\n                                                    <label for=\"\">Department<strong class=\"text-danger\">*</strong></label>\r\n                                                    <input v-model=\"department\" type=\"text\" class=\"form-control\" placeholder=\"Enter department\">\r\n                                                </div>\r\n                                            </div>\r\n\r\n\r\n                                        </div>\r\n\r\n                                    <div v-if=\"message!==null\" class=\"alert alert-danger\" role=\"alert\">\r\n                                        {{message}}\r\n                                    </div>\r\n\r\n\r\n                                        <div class=\"row\">\r\n                                            <div class=\"col-lg-10\">\r\n                                                <button  @click=\"addDepartment()\" type=\"submit\" class=\"btn btn-primary\"><i class=\"mdi mdi-account-plus-outline me-1\"></i>Save</button>\r\n                                            </div>\r\n                                        </div>\r\n\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div class=\"col-6\">\r\n                            <div class=\"card p-4\">\r\n\r\n                                <div>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"\">All Departments({{departments.length}})<strong class=\"text-danger\">*</strong></label>\r\n\r\n                                        <table class=\"table table-striped table-sm font-12 sources-table\">\r\n                                            <thead>\r\n                                            <tr class=\"font-weight-bold text-black\">\r\n                                                <th scope=\"col\">No</th>\r\n                                                <th scope=\"col\">Department</th>\r\n\r\n                                            </tr>\r\n                                            </thead>\r\n                                            <tbody>\r\n                                            <tr style=\"cursor: pointer;\" v-for=\"(item, index) in departments\" :value=\"item\" :key=\"index\" @click=\"selectedDepartment(item.department,item.id)\">\r\n                                                <td scope=\"row\">{{index+1}}.</td>\r\n\r\n                                                <td>{{item.department}}</td>\r\n                                            </tr>\r\n                                            <tr style=\"background: #cee9d8;\">\r\n                                                <th><strong>Total {{departments.length}}</strong></th>\r\n                                                <th></th>\r\n                                            </tr>\r\n                                            </tbody>\r\n                                        </table>\r\n\r\n<!--                                        <ul>-->\r\n<!--                                            <li style=\"cursor: pointer;\" v-for=\"(item, index) in departments\" :key=\"`${index}`\" @click=\"selectedDepartment(item.department,item.id)\" > {{item.department}}</li>-->\r\n<!--                                        </ul>-->\r\n                                    </div>\r\n                                </div>\r\n\r\n                            </div>\r\n                        </div>\r\n\r\n                    </div>\r\n\r\n\r\n                    <!-- end row -->\r\n                </div>\r\n                <!-- container-fluid -->\r\n            </div>\r\n            <!-- End Page-content -->\r\n\r\n            <footer class=\"footer \">\r\n                <div class=\"container-fluid \">\r\n                    <div class=\"row \">\r\n                        <div class=\"col-sm-6 \">\r\n                            Â© Best Birdy\r\n    </div>\r\n    <div class=\"col-sm-6 \">\r\n        <div class=\"text-sm-end d-sm-block \">\r\n            Best Birdy Billing system.\r\n        </div>\r\n    </div>\r\n    </div>\r\n    </div>\r\n    </footer>\r\n    </div>\r\n    <!-- end main content-->\r\n\r\n    </div>\r\n\r\n    </body>\r\n</template>\r\n\r\n<script>\r\n    import NavigationBar from \"@/components/Navigation\";\r\n    import router from \"@/router\";\r\n    import {biller, execute} from \"@/api\";\r\n    export default {\r\n        name: \"AddUser\",\r\n        components: {NavigationBar},\r\n        data() {\r\n            return{\r\n                permission:\"\",\r\n                message: null,\r\n                id:'',\r\n                update:false,\r\n                departments:[],\r\n                department:''\r\n            }\r\n        },\r\n        computed: {\r\n\r\n        },\r\n        watch: {\r\n\r\n        },\r\n\r\n        mounted() {\r\n            this.permission = sessionStorage.getItem(\"permission\")\r\n            this.getDepartment()\r\n        },\r\n        methods:{\r\n            gotTo(route){\r\n                router.push(route)\r\n            },\r\n            selectedDepartment(department,id){\r\n                this.update = true\r\n                this.department = department\r\n                this.id = id\r\n\r\n            },\r\n            addDepartment(){\r\n                if(this.department===''){\r\n                   this.message ='Fill in department'\r\n                }\r\n                this.message = null\r\n                const data = new FormData();\r\n                data.append(\"function\", \"addDepartment\");\r\n                data.append(\"department\", this.department);\r\n                data.append(\"update\", this.update);\r\n                data.append(\"id\", this.id);\r\n                execute(data,biller)\r\n                    .then((res) =>{\r\n                        if (res.data.success) {\r\n                            this.department =''\r\n                            this.update = false\r\n                            this.getDepartment()\r\n                        }else{\r\n                            this.message =res.data.message\r\n                        }\r\n                    })\r\n                    .catch((e) => {\r\n                        //alert(e.message);\r\n                        this.message =e.message\r\n                    });\r\n            },\r\n            getDepartment(){\r\n                this.message = null\r\n                const data = new FormData();\r\n                data.append(\"function\", \"getDepartment\");\r\n                execute(data,biller)\r\n                    .then((res) =>{\r\n                        if (res.data.success) {\r\n                            this.departments = res.data.data.departments\r\n\r\n\r\n                        }else{\r\n                            this.message =res.data.message\r\n                        }\r\n                    })\r\n                    .catch((e) => {\r\n                        //alert(e.message);\r\n                        this.message =e.message\r\n                    });\r\n            }\r\n\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AAuKI,OAAOA,aAAY,MAAO,yBAAyB;AACnD,OAAOC,MAAK,MAAO,UAAU;AAC7B,SAAQC,MAAM,EAAEC,OAAO,QAAO,OAAO;AACrC,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IAACL;EAAa,CAAC;EAC3BM,IAAIA,CAAA,EAAG;IACH,OAAM;MACFC,UAAU,EAAC,EAAE;MACbC,OAAO,EAAE,IAAI;MACbC,EAAE,EAAC,EAAE;MACLC,MAAM,EAAC,KAAK;MACZC,WAAW,EAAC,EAAE;MACdC,UAAU,EAAC;IACf;EACJ,CAAC;EACDC,QAAQ,EAAE,CAEV,CAAC;EACDC,KAAK,EAAE,CAEP,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACR,UAAS,GAAIS,cAAc,CAACC,OAAO,CAAC,YAAY;IACrD,IAAI,CAACC,aAAa,EAAC;EACvB,CAAC;EACDC,OAAO,EAAC;IACJC,KAAKA,CAACC,KAAK,EAAC;MACRpB,MAAM,CAACqB,IAAI,CAACD,KAAK;IACrB,CAAC;IACDE,kBAAkBA,CAACX,UAAU,EAACH,EAAE,EAAC;MAC7B,IAAI,CAACC,MAAK,GAAI,IAAG;MACjB,IAAI,CAACE,UAAS,GAAIA,UAAS;MAC3B,IAAI,CAACH,EAAC,GAAIA,EAAC;IAEf,CAAC;IACDe,aAAaA,CAAA,EAAE;MACX,IAAG,IAAI,CAACZ,UAAU,KAAG,EAAE,EAAC;QACrB,IAAI,CAACJ,OAAM,GAAG,oBAAmB;MACpC;MACA,IAAI,CAACA,