{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavigationBar from \"@/components/Navigation\";\nimport router from \"@/router\";\nimport { biller, execute } from \"@/api\";\nexport default {\n  name: \"AddUser\",\n  components: {\n    NavigationBar\n  },\n  data() {\n    return {\n      permission: \"\",\n      message: null,\n      id: '',\n      update: false,\n      departments: [],\n      department: ''\n    };\n  },\n  computed: {},\n  watch: {},\n  mounted() {\n    this.permission = sessionStorage.getItem(\"permission\");\n    this.getDepartment();\n  },\n  methods: {\n    gotTo(route) {\n      router.push(route);\n    },\n    selectedDepartment(department, id) {\n      this.update = true;\n      this.department = department;\n      this.id = id;\n    },\n    addDepartment() {\n      if (this.department === '') {\n        this.message = 'Fill in department';\n      }\n      this.message = null;\n      const data = new FormData();\n      data.append(\"function\", \"addDepartment\");\n      data.append(\"department\", this.department);\n      data.append(\"update\", this.update);\n      data.append(\"id\", this.id);\n      execute(data, biller).then(res => {\n        if (res.data.success) {\n          this.department = '';\n          this.update = false;\n          this.getDepartment();\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    },\n    getDepartment() {\n      this.message = null;\n      const data = new FormData();\n      data.append(\"function\", \"getDepartment\");\n      execute(data, biller).then(res => {\n        if (res.data.success) {\n          this.departments = res.data.data.departments;\n        } else {\n          this.message = res.data.message;\n        }\n      }).catch(e => {\n        //alert(e.message);\n        this.message = e.message;\n      });\n    }\n  }\n};","map":{"version":3,"na